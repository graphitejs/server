{
  "name": "@graphite/server",
  "version": "0.0.0",
  "description": "GraphiteJS Framework",
  "scripts": {
    "lint": "eslint -f table ./src/** ./test/**",
    "lint:fix": "eslint --fix ./src/** ./test/**",
    "test": "babel-node ./test",
    "test:unit": "",
    "test:integration": "",
    "test:functional": "babel-node ./test/functional",
    "coverage": "nyc --report-dir ./.coverage-functional npm run test",
    "coverage:unit": "",
    "coverage:integration": "",
    "coverage:functional": "nyc --report-dir ./.coverage-functional npm run test:functional",
    "compile": "babel src --out-dir dist",
    "prepublish": "npm run compile",
    "start:example": "babel-node ./examples/TodoList"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.11.1",
    "eslint-plugin-prettier": "3.0.1",
    "isomorphic-fetch": "2.2.1",
    "nyc": "13.1.0",
    "prettier": "1.15.3",
    "riteway": "4.0.1"
  },
  "dependencies": {
    "apollo-server": "2.3.1",
    "apollo-server-express": "2.3.1",
    "express": "4.16.4",
    "graphql": "14.0.2",
    "pino": "5.10.5",
    "pino-pretty": "2.5.0"
  }
}
