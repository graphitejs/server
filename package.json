{
  "name": "@graphite/server",
  "version": "0.9.32",
  "description": "GraphiteJS Framework",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint -f table ./src/** ./test/**",
    "lint:fix": "eslint --fix ./src/** ./test/** ./examples/**",
    "test": "babel-node ./test",
    "test:unit": "babel-node ./test/unit",
    "test:integration": "",
    "test:functional": "babel-node ./test/functional",
    "coverage": "nyc --report-dir ./.coverage npm run test",
    "coverage:unit": "nyc --report-dir ./.coverage-unit npm run test:unit",
    "coverage:integration": "",
    "coverage:functional": "nyc --report-dir ./.coverage-functional npm run test:functional",
    "compile": "babel src --out-dir dist",
    "prepublish": "npm run compile",
    "codecov": "codecov",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "start:example": "babel-node ./examples/TodoList"
  },
  "author": "Walter Zalazar",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/node": "7.6.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.6.3",
    "@babel/register": "7.6.2",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.5.4",
    "@semantic-release/npm": "5.2.0",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link-ws": "1.0.19",
    "babel-eslint": "10.0.3",
    "codecov": "3.6.1",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "1.3.1",
    "isomorphic-fetch": "2.2.1",
    "lint-staged": "8.2.1",
    "mongodb": "3.3.3",
    "mongodb-memory-server": "5.2.11",
    "nyc": "13.3.0",
    "prettier": "1.18.2",
    "riteway": "6.1.0",
    "semantic-release": "15.13.27",
    "subscriptions-transport-ws": "0.9.16",
    "travis-deploy-once": "5.0.11",
    "ws": "6.2.1"
  },
  "dependencies": {
    "apollo-server": "2.9.7",
    "apollo-server-express": "2.9.7",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "pino": "5.13.5",
    "pino-pretty": "2.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
